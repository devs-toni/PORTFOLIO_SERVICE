{
  "name": "arcprojects",
  "version": "0.1.2-SNAPSHOT.0",
  "homepage": "/",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "build:int": "env-cmd -f .env.integration npm run build && npm run zip-int",
    "build:prod": "npm run build && npm run zip-prod",
    "zip-int": "cd dist; zip -r ../arcprojects-v2_${npm_package_version}_int.zip * .htaccess",
    "zip-prod": "cd dist; zip -r ../arcprojects-v2_$npm_package_version.zip * .htaccess"
  },
  "dependencies": {
    "aos": "^3.0.0-beta.6",
    "env-cmd": "^10.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-router-dom": "^6.8.1",
    "react-transition-group": "^4.4.5",
    "react-uuid": "^2.0.0",
    "sweetalert2": "^11.7.3",
    "sweetalert2-react-content": "^5.0.7",
    "tw-elements": "^1.0.0-beta1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.6",
    "vite": "^4.1.1"
  }
}
